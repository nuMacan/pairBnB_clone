<%= link_to "Listing Index", listings_path %>
<br>

<table>
  <tr>
    <th>Name</th>
    <th>City</th>
    <th>Price</th>
  </tr>


<% @listings.each do |l| %>
  <tr> 
    <td><%= link_to l.listing_name, listing_path(l) %>
      </td>
    <td><%= l.city %></td>
    <td><%= l.price %></td>
    <td><%= link_to 'Show More', listing_path(l) %></td>

  </tr>
<% end %>
</table>

<br><br>

<%= paginate @listings %> 
<%= page_entries_info @listings %>